SELECT 
  "group",
  COUNT(*) AS total_agents,
  SUM(CASE 
        WHEN downloaded_invoice IS NOT NULL AND downloaded_invoice <> 0 THEN 1 
        ELSE 0 
      END) AS downloaded_count,
  ROUND(
    100.0 * SUM(CASE 
                  WHEN downloaded_invoice IS NOT NULL AND downloaded_invoice <> 0 THEN 1 
                  ELSE 0 
                END) 
    / COUNT(*), 
    2
  ) AS pct_agents_downloaded_invoice
FROM ab_test_agents_dataset
GROUP BY "group"
ORDER BY "group";
